{
  "name": "site-hderma",
  "projectType": "application",
  "sourceRoot": "apps/site/hderma/src",
  "prefix": "taza-base",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "allowedCommonJsDependencies": [
          "crypto-js",
          "moment",
          "aos",
          "file-saver"
        ],
        "outputPath": "dist/browser",
        "index": "apps/site/hderma/src/index.html",
        "main": "apps/site/hderma/src/main.ts",
        "polyfills": "apps/site/hderma/src/polyfills.ts",
        "tsConfig": "apps/site/hderma/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/site/hderma/src/favicon.ico",
          "apps/site/hderma/src/assets",
          "apps/site/hderma/src/robots.txt",
          "apps/site/hderma/src/sitemap.xml",
          "apps/site/hderma/src/sitemap_baiviet.xml",
          "apps/site/hderma/src/sitemap_root.xml",
          "apps/site/hderma/src/sitemap_sanpham.xml",
          "apps/site/hderma/src/manifest.webmanifest",
          "apps/site/hderma/src/firebase-messaging-sw.js"
        ],
        "styles": [
          "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
          "apps/site/hderma/src/styles.scss",
          "./node_modules/ngx-spinner/animations/ball-spin-clockwise.css",
          "./node_modules/ngx-spinner/animations/ball-clip-rotate-multiple.css"
        ],
        "scripts": [],
        "serviceWorker": true,
        "ngswConfigPath": "apps/site/hderma/ngsw-config.json"
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "5mb",
              "maximumError": "10mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "200kb",
              "maximumError": "400kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/site/hderma/src/environments/environments.ts",
              "with": "apps/site/hderma/src/environments/environments.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": true,
          "optimization": true,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "site-hderma:build:production"
        },
        "development": {
          "browserTarget": "site-hderma:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "site-hderma:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/site/hderma/**/*.ts",
          "apps/site/hderma/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/site/hderma"],
      "options": {
        "jestConfig": "apps/site/hderma/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "server": {
      "dependsOn": ["build"],
      "executor": "@angular-devkit/build-angular:server",
      "options": {
        "outputPath": "dist/server",
        "main": "apps/site/hderma/server.ts",
        "tsConfig": "apps/site/hderma/tsconfig.server.json"
      },
      "configurations": {
        "production": {
          "outputHashing": "media",
          "fileReplacements": [
            {
              "replace": "apps/site/hderma/src/environments/environments.ts",
              "with": "apps/site/hderma/src/environments/environments.prod.ts"
            }
          ]
        },
        "development": {
          "optimization": false,
          "sourceMap": true,
          "extractLicenses": false
        }
      },
      "defaultConfiguration": "production"
    },
    "serve-ssr": {
      "executor": "@nguniversal/builders:ssr-dev-server",
      "configurations": {
        "development": {
          "browserTarget": "site-hderma:build:development",
          "serverTarget": "site-hderma:server:development"
        },
        "production": {
          "browserTarget": "site-hderma:build:production",
          "serverTarget": "site-hderma:server:production"
        }
      },
      "defaultConfiguration": "development"
    },
    "prerender": {
      "executor": "@nguniversal/builders:prerender",
      "options": {
        "routes": ["/"]
      },
      "configurations": {
        "development": {
          "browserTarget": "site-hderma:build:development",
          "serverTarget": "site-hderma:server:development"
        },
        "production": {
          "browserTarget": "site-hderma:build:production",
          "serverTarget": "site-hderma:server:production"
        }
      },
      "defaultConfiguration": "production"
    }
  },
  "tags": []
}
